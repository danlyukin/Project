
car_brands = {}


workbook = openpyxl.load_workbook('cars.xlsx')


sheet = workbook.active


for row in sheet.iter_rows(values_only=True):
    
    for make in row:
  
        if make:
ъ
            if make in car_makes:
                car_makes[make] += 1
            
            else:
                car_makes[make] = 1


workbook.close()


most_popular_makes = sorted(car_makes.items(), key=lambda x: x[1], reverse=True)[:5]


for brand, count in most_popular_makes:
    print(f"{brand}: {count} упоминаний")
